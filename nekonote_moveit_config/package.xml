<?xml version="1.0"?>
<package>

  <name>nekonote_moveit_config</name>
  <version>0.1.7</version>
  <description>
     An automatically generated package with all the configuration and launch files for using the nekonote with the MoveIt Motion Planning Framework
  </description>

  <maintainer email="assistant@moveit.ros.org">MoveIt Setup Assistant</maintainer>
  <maintainer email="dev@rt-net.jp">Noriaki Nakagawa</maintainer>

  <license>BSD</license>

  <!-- <url type="website">http://ros.org/wiki/nekonote_moveit_config/</url> -->
  <url type="bugtracker">https://github.com/rt-net/ros_nekonote/issues</url>
  <url type="repository">https://github.com/rt-net/ros_nekonote</url>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>rospy</build_depend>
  <build_depend>actionlib</build_depend>
  <build_depend>actionlib_msgs</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>control_msgs</build_depend>
  <build_depend>trajectory_msgs</build_depend>
  <run_depend>rospy</run_depend>
  <run_depend>actionlib</run_depend>
  <run_depend>actionlib_msgs</run_depend>
  <run_depend>sensor_msgs</run_depend>
  <run_depend>control_msgs</run_depend>
  <run_depend>trajectory_msgs</run_depend>

  <run_depend>moveit_ros_move_group</run_depend>
  <run_depend>moveit_planners_ompl</run_depend>
  <run_depend>moveit_ros_visualization</run_depend>
  <run_depend>joint_state_publisher</run_depend>
  <run_depend>robot_state_publisher</run_depend>
  <run_depend>xacro</run_depend>
  <build_depend>nekonote_description</build_depend>
  <run_depend>nekonote_description</run_depend>
  <run_depend>moveit_simple_controller_manager</run_depend>

</package>
